            <xs:element minOccurs="0" maxOccurs="1" name="indeks-dolny" type="indeks-dolny-typ">
              <xs:annotation>
                <xs:documentation>Treść pisana w indeksie dolnym.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="1" name="wytluszczenie" type="wytluszczenie-typ">
              <xs:annotation>
                <xs:documentation>Treść pisana w wytłuszczoną czcionką.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="1" name="kursywa" type="kursywa-typ">
              <xs:annotation>
                <xs:documentation>Treść pisana kursywą.</xs:documentation>
