  <xs:complexType name="akapit-typ" mixed="true">
    <xs:annotation>
      <xs:documentation>Akapit - Treść pisana od nowego wiersza oraz zawartość komórek tabeli.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="jednostka-redakcyjna-typ">
        <xs:sequence>
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element minOccurs="0" maxOccurs="1" name="nbsp" type="nbsp-typ">
              <xs:annotation>
                <xs:documentation>Oznaczenie spacji nierozdzielającej.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="1" name="nl" type="nl-typ">
