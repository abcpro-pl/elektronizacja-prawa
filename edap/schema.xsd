				<xsd:element ref="tresc-przypisow" minOccurs="0"/>
				<xsd:element ref="zalacznik_bin" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:attributeGroup ref="atr-wspolne"/>
		</xsd:complexType>
	</xsd:element>
