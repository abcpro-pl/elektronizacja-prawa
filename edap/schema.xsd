		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="funkcja">
				<xsd:annotation>
					<xsd:documentation>Funkcja/stanowisko osoby podpisujacej.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="tytul">
				<xsd:annotation>
					<xsd:documentation>Tytul naukowy/zawodowy osoby podpisujacej.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="imie">
				<xsd:annotation>
					<xsd:documentation>Imie/imiona osoby podpisujacej.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="nazwisko">
				<xsd:annotation>
					<xsd:documentation>Nazwisko osoby podpisujacej.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attributeGroup ref="atr-wizualizacyjne"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="zatwierdzil">
		<xsd:annotation>
			<xsd:documentation>Tekstowa informacja o osobie odpowiedzialnej za zatwierdzenie aktu.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true"/>
	</xsd:element>
	<xsd:element name="osoba">
		<xsd:annotation>
			<xsd:documentation>Osoba.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:group ref="elem-komentarza"/>
			</xsd:sequence>
			<xsd:attribute name="tytul_nauk" use="optional">
				<xsd:annotation>
					<xsd:documentation>Tytul (naukowy)</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="nazwisko" use="required">
				<xsd:annotation>
					<xsd:documentation>Nazwisko</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="imie" use="required">
				<xsd:annotation>
					<xsd:documentation>Imie</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="funkcja">
				<xsd:annotation>
					<xsd:documentation>funkcja w zespole</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="sklad">
		<xsd:annotation>
			<xsd:documentation>Lista sedziow orzekajacych.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence maxOccurs="unbounded">
				<xsd:element ref="osoba"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!--  P r z y p i s y  -->
	<xsd:element name="przypis">
		<xsd:annotation>
			<xsd:documentation>Odnosnik do przypisu.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="nr">
				<xsd:annotation>
					<xsd:documentation>Numer (oznaczenie) przypisu.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="tresc-przypisow">
		<xsd:annotation>
			<xsd:documentation>Element grupujacy tresci przypisow.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="tresc-przypisu" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="tresc-przypisu">
		<xsd:annotation>
			<xsd:documentation>Tresc pojedynczego przypisu.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="elem-tekstowe"/>
				<xsd:element ref="ustep-przypisu"/>
				<xsd:element ref="punkt-przypisu"/>
				<xsd:element ref="litera-przypisu"/>
				<xsd:element ref="tiret-przypisu"/>
				<xsd:element ref="adres-publikacji"/>
				<xsd:element ref="dyrektywa"/>
			</xsd:choice>
			<xsd:attribute name="nr">
				<xsd:annotation>
					<xsd:documentation>Numer (oznaczenie) przypisu, ktorego tresc reprezentuje dany element.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ustep-przypisu">
		<xsd:annotation>
			<xsd:documentation>Ustep w podstrukturze przypisu.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="elem-tekstowe"/>
				<xsd:element ref="punkt-przypisu"/>
				<xsd:element ref="litera-przypisu"/>
				<xsd:element ref="tiret-przypisu"/>
				<xsd:element ref="dyrektywa"/>
			</xsd:choice>
			<xsd:attributeGroup ref="atr-wspolne-nr"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="punkt-przypisu">
		<xsd:annotation>
			<xsd:documentation>Punkt w postrukturze przypisu.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="elem-tekstowe"/>
				<xsd:element ref="litera-przypisu"/>
				<xsd:element ref="tiret-przypisu"/>
				<xsd:element ref="dyrektywa"/>
			</xsd:choice>
			<xsd:attributeGroup ref="atr-wspolne-nr"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="litera-przypisu">
		<xsd:annotation>
			<xsd:documentation>Litera w podstrukturze przypisu.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="elem-tekstowe"/>
				<xsd:element ref="tiret-przypisu"/>
				<xsd:element ref="dyrektywa"/>
			</xsd:choice>
			<xsd:attributeGroup ref="atr-wspolne-nr"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="tiret-przypisu">
		<xsd:annotation>
			<xsd:documentation>Tiret w podstrukturze przypisu.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="elem-tekstowe"/>
				<xsd:element ref="dyrektywa"/>
			</xsd:choice>
			<xsd:attributeGroup ref="atr-wspolne"/>
		</xsd:complexType>
	</xsd:element>
	<!--  P r z y w o l a n i e  -->
	<xsd:element name="przywolanie">
		<xsd:annotation>
			<xsd:documentation>Specjalny rodzaj linku do przywolywanego aktu.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:sequence>
				<xsd:element ref="metryka" minOccurs="0"/>
				<xsd:element ref="przypis" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="akt-id" use="required">
				<xsd:annotation>
					<xsd:documentation>Identyfikator aktu.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="typ" default="nowelizacyjne">
				<xsd:annotation>
					<xsd:documentation>Rodzaj przywolania (nowelizacyjne, informacyjne, cytujace, odsylajace).</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="nowelizacyjne"/>
						<xsd:enumeration value="informacyjne"/>
						<xsd:enumeration value="cytujace"/>
						<xsd:enumeration value="odsylajace"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="zamrozone" default="nie">
				<xsd:annotation>
					<xsd:documentation>Informacja o rezygnacji z aktualizacji przywolan (dla tekstow zarchiwizowanych).</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="tak"/>
						<xsd:enumeration value="nie"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!--  E l e m e n t y   f o r m a t o w a n i a  -->
	<xsd:element name="wytluszczenie">
		<xsd:annotation>
			<xsd:documentation>Oznaczenie tekstu wytluszczonego.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="elem-wewnetrzne"/>
			</xsd:choice>
			<xsd:attributeGroup ref="atr-wspolne"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="kursywa">
		<xsd:annotation>
			<xsd:documentation>Oznaczenie tekstu zapisanego kursywa.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="elem-wewnetrzne"/>
			</xsd:choice>
			<xsd:attributeGroup ref="atr-wspolne"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="podkreslenie">
		<xsd:annotation>
			<xsd:documentation>Oznaczenie tekstu podkreslonego.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="elem-wewnetrzne"/>
			</xsd:choice>
			<xsd:attributeGroup ref="atr-wspolne"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="wyspa">
		<xsd:annotation>
			<xsd:documentation>Element umozliwiajacy zapis dowolnego fragmentu tekstu wykraczajacego struktura badz formatowaniem poza niniejszy schemat.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:attribute name="opis" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Tekstowy opis zawartosci wyspy.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="typ">
				<xsd:annotation>
					<xsd:documentation>Format zapisu tekstu wewnatrz wyspy.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="odf"/>
						<xsd:enumeration value="ooxml"/>
						<xsd:enumeration value="xhtml"/>
						<xsd:enumeration value="latex"/>
						<xsd:enumeration value="rtf"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="zalacznik_bin">
		<xsd:annotation>
			<xsd:documentation>Element umozliwiajacy zapis dowolnego pliku, jako czesc aktu.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:attribute name="nazwa" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Nazwa pliku.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="hash" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Skr�t kryptograficzny pliku.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="typ">
				<xsd:annotation>
					<xsd:documentation>Funkcja skr�tu.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="MD5"/>
						<xsd:enumeration value="SHA-1"/>
						<xsd:enumeration value="SHA-2"/>
						<xsd:enumeration value="RIPEMD-160"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- C y t o w a n i e   b e z p o s r e d n i e  -->
	<xsd:element name="cytuj">
		<xsd:annotation>
			<xsd:documentation>Element umozliwiajacy bezposrednie cytowanie przepisow.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<!-- dodane na wniosek TK -->
				<xsd:group ref="elem-wewnetrzne"/>
				<xsd:group ref="elem-komentarza"/>
				<xsd:element ref="tabela"/>
			</xsd:choice>
			<xsd:attributeGroup ref="atr-wspolne"/>
		</xsd:complexType>
	</xsd:element>
	<!--  Z n a c z n i k i   n o w e l i z a c y j n e   i   c y t o w a n i e  -->
	<xsd:element name="dodaj">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy dodanie przepisow.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice>
				<xsd:sequence>
					<xsd:element ref="po-elemencie" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="element"/>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element ref="w-elemencie" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="po-tekscie"/>
					<xsd:element ref="akcja" minOccurs="0"/>
					<xsd:sequence>
						<xsd:element ref="tekst" minOccurs="0"/>
						<xsd:element ref="element" minOccurs="0"/>
					</xsd:sequence>
				</xsd:sequence>
			</xsd:choice>
			<xsd:attributeGroup ref="atr-nowelizacyjne"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="uchyl">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy uchylenie przepisow.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:choice minOccurs="0">
					<xsd:choice maxOccurs="unbounded">
						<xsd:element ref="element"/>
						<xsd:element ref="elementy"/>
					</xsd:choice>
					<xsd:sequence>
						<xsd:element ref="w-elemencie" minOccurs="0" maxOccurs="unbounded"/>
						<xsd:element ref="tekst"/>
					</xsd:sequence>
					<xsd:element ref="oprocz" maxOccurs="unbounded"/>
				</xsd:choice>
			</xsd:sequence>
			<xsd:attributeGroup ref="atr-nowelizacyjne"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="zastap">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy zmiane brzmienia przepisow.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice>
				<xsd:sequence>
					<xsd:choice maxOccurs="unbounded">
						<xsd:element ref="element"/>
						<xsd:element ref="elementy"/>
					</xsd:choice>
					<xsd:element ref="elementem" minOccurs="0"/>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element ref="w-elemencie" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="tekst"/>
					<xsd:element ref="akcja" minOccurs="0"/>
					<xsd:element ref="tekstem"/>
				</xsd:sequence>
			</xsd:choice>
			<xsd:attributeGroup ref="atr-nowelizacyjne"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="cytowanie">
		<xsd:annotation>
			<xsd:documentation>Element umozliwiajacy cytowanie przepisow.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice>
				<xsd:element ref="element"/>
				<xsd:element ref="cytowanie"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="po-elemencie">
		<xsd:annotation>
			<xsd:documentation>Element wskazujacy sciezke do elementu struktury, po ktorym maja zostac dodane nowe przepisy.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="id" use="required">
				<xsd:annotation>
					<xsd:documentation>Sciezka w postaci wyrazenia XPath identyfikujaca element.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="w-elemencie">
		<xsd:annotation>
			<xsd:documentation>Element wskazujacy sciezke do elementu struktury, w ktorym maja zostac zmienione przepisy.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="id" use="required">
				<xsd:annotation>
					<xsd:documentation>Sciezka w postaci wyrazenia XPath identyfikujaca element.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="w-zdaniu">
				<xsd:annotation>
					<xsd:documentation>Numer zdania, w ktorym ma nastapic zamiana tekstu.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="element">
		<xsd:annotation>
			<xsd:documentation>Element wskazujacy sciezke do elementu struktury, w ktorym ma zostac znowelizowana tresc lub zawierajacy nowe brzmienie dodawanych przepisow.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="elem-struktury">
					<xsd:attribute name="id">
						<xsd:annotation>
							<xsd:documentation>Sciezka w postaci wyrazenia XPath identyfikujaca element.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="elementy">
		<xsd:annotation>
			<xsd:documentation>Element wskazujacy sciezke do ciagu elementow nowelizowanej struktury.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="od" use="required">
				<xsd:annotation>
					<xsd:documentation>Sciezka w postaci wyrazenia XPath identyfikujaca element poczatkowy.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="do" use="required">
				<xsd:annotation>
					<xsd:documentation>Sciezka w postaci wyrazenia XPath identyfikujaca element koncowy.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="elementem">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy nowe brzmienie nowelizowanej czesci aktu.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="elem-struktury">
					<xsd:attribute name="poziom">
						<xsd:annotation>
							<xsd:documentation>Nazwa elementu obudowujacego nowa czesc struktury: paragraf lub ustep.</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="paragraf"/>
								<xsd:enumeration value="ustep"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="po-tekscie">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy fragment tekstu, po ktorym ma zostac dodana nowa tresc przepisu.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="elem-tekstowe"/>
			</xsd:choice>
			<xsd:attributeGroup ref="atr-identyfikacyjne"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="tekst">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy fragment tekstu, ktory ma zostac znowelizowany.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="elem-tekstowe"/>
			</xsd:choice>
			<xsd:attributeGroup ref="atr-identyfikacyjne"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="akcja">
		<xsd:annotation>
			<xsd:documentation>Element reprezentuacy fragment tekstu pomiedzy starym i nowym brzmieniem zmienianego tekstu.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="elem-tekstowe"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="tekstem">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy nowe wyrazy przepisu zmieniajacego brzmienie.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="elem-tekstowe"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="oprocz">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy strukture pozostawiana w akcie po uchyleniu go w calosci.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:choice maxOccurs="unbounded">
					<xsd:element ref="element"/>
					<xsd:element ref="elementy"/>
				</xsd:choice>
			</xsd:sequence>
			<xsd:attribute name="do" type="data" use="required">
				<xsd:annotation>
					<xsd:documentation>Data konca obowiazywania pozostawionych przepisow.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!--  Z n a c z n i k i   i n t e r p r e t a c y j n e  -->
	<xsd:element name="trybunal">
		<xsd:annotation>
			<xsd:documentation>Element umozliwiajacy oznaczenie w tekscie ustawy wyrokow Trybunalu Konstytucyjnego.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="adres-dzu" minOccurs="0"/>
				<xsd:element ref="zakwestionowany" minOccurs="0"/>
				<xsd:element ref="komentarz" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="stan" use="required">
				<xsd:annotation>
					<xsd:documentation>Stan procesu rozpatrywania zgodnosci przepisow z Konstytucja: "zaskarzony" lub "po wyroku".</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="zaskarzony"/>
						<xsd:enumeration value="po-wyroku"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="zakwestionowany">
		<xsd:annotation>
			<xsd:documentation>Element obudowujacy zaskarzone przepisy.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:sequence>
				<xsd:group ref="rozsz-elem-wewnetrzne"/>
				<xsd:element ref="ustep" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="paragraf" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="punkt" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="litera" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="tiret" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:group ref="elem-komentarza"/>
			</xsd:sequence>
			<xsd:attributeGroup ref="atr-wspolne"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="komentarz">
		<xsd:annotation>
			<xsd:documentation>Tekstowy komentarz do obowiazujacego stanu prawnego.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true"/>
	</xsd:element>
	<xsd:element name="wejscie-w-zycie">
		<xsd:annotation>
			<xsd:documentation>Informacja o dacie wejscia przepisow w zycie.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="od">
					<xsd:annotation>
						<xsd:documentation>Reprezentacja grupy przepisow o wspolnej dacie wejscia w zycie.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="stosowanie">
		<xsd:annotation>
			<xsd:documentation>Informacja o dacie stosowania przepisow.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="od">
					<xsd:annotation>
						<xsd:documentation>Reprezentacja grupy przepisow o wspolnej dacie poczatku stosowania.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="od">
		<xsd:annotation>
			<xsd:documentation>Reprezentacja grupy przepisow o wspolnej dacie wejscia w zycie/poczatku stosowania.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="element"/>
				<xsd:element ref="elementy"/>
			</xsd:choice>
			<xsd:attribute name="data">
				<xsd:annotation>
					<xsd:documentation>Data poczatku stosowania/wejscia w zycie przepisow.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="przepisy-wykonawcze">
		<xsd:annotation>
			<xsd:documentation>Element wskazujacy miejsce umieszczenia przepisow wykonawczych.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true"/>
	</xsd:element>
	<xsd:element name="dyrektywa">
		<xsd:annotation>
			<xsd:documentation>Przywolanie dyrektywy UE.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="tytul"/>
				<xsd:element ref="adres-publikacji-ue" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="nr" type="xsd:positiveInteger">
				<xsd:annotation>
					<xsd:documentation>Numer dyrektywy.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="data" type="data">
				<xsd:annotation>
					<xsd:documentation>Data wydania dyrektywy.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="adres-publikacji-ue">
		<xsd:annotation>
			<xsd:documentation>Adres publikacji aktu w Dzienniku Urzedowym UE.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="adres-dzu-ue" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="adres-dzu-ue">
		<xsd:annotation>
			<xsd:documentation>Adres Dziennika Urzedowego UE.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="seria">
				<xsd:annotation>
					<xsd:documentation>Seria Dziennika Urzedowego UE.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="L"/>
						<xsd:enumeration value="C"/>
						<xsd:enumeration value="S"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="nr" type="xsd:positiveInteger">
				<xsd:annotation>
					<xsd:documentation>Numer Dziennika.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="data" type="data">
				<xsd:annotation>
					<xsd:documentation>Data wydania Dziennika.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!--  L i n k i  -->
	<xsd:element name="link">
		<xsd:annotation>
			<xsd:documentation>Link ogolnego przeznaczenia.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:attribute name="cel" use="required">
				<xsd:annotation>
					<xsd:documentation>Sciezka/wskazanie elementu docelowego linku.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="typ" default="hiperlink">
				<xsd:annotation>
					<xsd:documentation>Typ linku (hiperlink, link do zalacznika, link do definicji).</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="hiperlink"/>
						<xsd:enumeration value="zalacznik"/>
						<xsd:enumeration value="definicja"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="mime">
				<xsd:annotation>
					<xsd:documentation>Typ MIME wlaczanej zawartosci.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="definicja">
		<xsd:annotation>
			<xsd:documentation>Oznaczenie miejsca wystapienia definicji.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:attribute name="pojecie" use="required">
				<xsd:annotation>
					<xsd:documentation>Definiowane pojecie.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!--  E l e m e n t y   g r u p u j a c e  -->
	<xsd:element name="fragment">
		<xsd:annotation>
			<xsd:documentation>Znakowy fragment tekstu.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="elem-wewnetrzne"/>
			</xsd:choice>
			<xsd:attributeGroup ref="atr-wspolne"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="blok">
		<xsd:annotation>
			<xsd:documentation>Akapitowy fragment tekstu.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="elem-wewnetrzne"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!--  T a b e l e  -->
	<xsd:element name="tabela">
		<xsd:annotation>
			<xsd:documentation>Tabela.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="naglowek-tabeli" minOccurs="0"/>
				<xsd:element ref="wiersz" maxOccurs="unbounded"/>
				<xsd:element ref="stopka-tabeli" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="naglowek-tabeli">
		<xsd:annotation>
			<xsd:documentation>Naglowek tabeli.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="wiersz" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="stopka-tabeli">
		<xsd:annotation>
			<xsd:documentation>Stopka tabeli.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="wiersz" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="wiersz">
		<xsd:annotation>
			<xsd:documentation>Wiersz tabeli.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice maxOccurs="unbounded">
				<xsd:element ref="komorka"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="komorka">
		<xsd:annotation>
			<xsd:documentation>Pojedyncza komorka.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="elem-tekstowe"/>
			</xsd:choice>
			<xsd:attribute name="scal-poziomo" type="liczba" default="1">
				<xsd:annotation>
					<xsd:documentation>Liczba kolumn, na ktore rozciaga sie komorka.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="scal-pionowo" type="liczba" default="1">
				<xsd:annotation>
					<xsd:documentation>Liczba wierszy, na ktore rozciaga sie komorka.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attributeGroup ref="atr-wyrownanie-poz"/>
			<xsd:attributeGroup ref="atr-wyrownanie-pion"/>
			<xsd:attribute name="szerokosc">
				<xsd:annotation>
					<xsd:documentation>Procentowa szerokosc komorki (kolumny).</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:pattern value="\d+%"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
