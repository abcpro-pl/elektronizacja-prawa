	<!--  G r u p y   a t r y b u t o w  -->
	<xsd:attributeGroup name="atr-czasowe">
		<xsd:annotation>
			<xsd:documentation>Atrybuty okreslajace poczatkowa i koncowa date obowiazywania elementu.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="obowiazuje-od" type="data">
			<xsd:annotation>
				<xsd:documentation>Data poczatku obowiazywania elementu.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="obowiazuje-do" type="data">
			<xsd:annotation>
				<xsd:documentation>Data konca obowiazywania elementu.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:attributeGroup>
	<xsd:attributeGroup name="atr-wizualizacyjne">
		<xsd:annotation>
			<xsd:documentation>Atrybuty sterujace wyswietlaniem elementu.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="widoczny" default="tak">
			<xsd:annotation>
				<xsd:documentation>Informacja, czy element ma byc wizualizowany.</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="tak"/>
					<xsd:enumeration value="nie"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:attributeGroup>
	<xsd:attributeGroup name="atr-sterujace">
		<xsd:annotation>
			<xsd:documentation>Atrybuty ogolnego przeznaczenia.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="status">
			<xsd:annotation>
				<xsd:documentation>Stan elementu - atrybut ogolnego przeznaczenia.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="komentarz">
			<xsd:annotation>
				<xsd:documentation>Komentarz do elementu - atrybut ogolnego przeznaczenia.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:attributeGroup>
	<xsd:attributeGroup name="atr-wspolne">
		<xsd:annotation>
			<xsd:documentation>Podstawowy zestaw atrybutow.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="uchylony" default="nie">
			<xsd:annotation>
				<xsd:documentation>Informacja, czy element zostal uchylony.</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="tak"/>
					<xsd:enumeration value="nie"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attributeGroup ref="atr-sterujace"/>
		<xsd:attributeGroup ref="atr-wizualizacyjne"/>
		<xsd:attributeGroup ref="atr-czasowe"/>
	</xsd:attributeGroup>
	<xsd:attributeGroup name="atr-wspolne-nr">
		<xsd:annotation>
			<xsd:documentation>Atrybuty dla elementow numerowanych.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="nr">
			<xsd:annotation>
				<xsd:documentation>Numer elementu struktury.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attributeGroup ref="atr-wspolne"/>
	</xsd:attributeGroup>
	<xsd:attributeGroup name="atr-identyfikacyjne">
		<xsd:annotation>
			<xsd:documentation>Atrybuty identyfikujace zakres nowelizacji tekstowej.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="wystapienie" default="ostatnie">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="pierwsze"/>
					<xsd:enumeration value="drugie"/>
					<xsd:enumeration value="ostatnie"/>
					<xsd:enumeration value="wszystkie"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:attributeGroup>
	<xsd:attributeGroup name="atr-nowelizacyjne">
		<xsd:annotation>
			<xsd:documentation>Atrybuty dla znacznikow nowelizacyjnych.</xsd:documentation>
		</xsd:annotation>
		<xsd:attributeGroup ref="atr-wizualizacyjne"/>
	</xsd:attributeGroup>
	<xsd:attributeGroup name="atr-wyrownanie-poz">
		<xsd:annotation>
			<xsd:documentation>Atrybuty sterujace wyrownaniem w poziomie.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="wyrownanie">
			<xsd:annotation>
				<xsd:documentation>Wyrownanie: do lewej, do prawej, wysrodkowane, wyjustowane.</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:token">
					<xsd:enumeration value="do-lewej"/>
					<xsd:enumeration value="do-prawej"/>
					<xsd:enumeration value="wysrodkowane"/>
					<xsd:enumeration value="wyjustowane"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:attributeGroup>
	<xsd:attributeGroup name="atr-wyrownanie-pion">
		<xsd:annotation>
			<xsd:documentation>Atrybuty sterujace wyrownaniem w pionie.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="wyrownanie-w-pionie">
			<xsd:annotation>
				<xsd:documentation>Wyrownanie: do gory, do dolu, wysrodkowane.</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:token">
					<xsd:enumeration value="do-gory"/>
					<xsd:enumeration value="wysrodkowane"/>
					<xsd:enumeration value="do-dolu"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:attributeGroup>
	<!--  T y p y   p r o s t e  -->
	<xsd:simpleType name="data">
		<xsd:annotation>
			<xsd:documentation>Wzorzec dat (RRRR-MM-DD).</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(19|20)[0-9]{2}\-(01|02|03|04|05|06|07|08|09|10|11|12)\-([0-2]{1}[0-9]{1}|30|31)"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="zakres_lat">
		<xsd:annotation>
			<xsd:documentation>Wzorzec zakresu lat (RRRR-RRR).</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(19|20)[0-9]{2}\-(19|20)[0-9]{2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="liczba">
		<xsd:annotation>
			<xsd:documentation>Wzorzec dla liczb naturalnych.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:nonNegativeInteger">
			<xsd:pattern value="[0-9]+"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--  F r a g m e n t y   d e f i n i c j i  -->
	<xsd:complexType name="elem-struktury">
		<xsd:annotation>
			<xsd:documentation>Element struktury.</xsd:documentation>
		</xsd:annotation>
		<xsd:choice minOccurs="0">
			<xsd:element ref="podstawa-prawna"/>
			<xsd:element ref="ksiega" maxOccurs="unbounded"/>
			<xsd:element ref="czesc" maxOccurs="unbounded"/>
			<xsd:element ref="tyt" maxOccurs="unbounded"/>
			<xsd:element ref="dzial" maxOccurs="unbounded"/>
			<xsd:element ref="rozdzial" maxOccurs="unbounded"/>
			<xsd:element ref="oddzial" maxOccurs="unbounded"/>
			<xsd:element ref="artykul" maxOccurs="unbounded"/>
			<xsd:element ref="paragraf" maxOccurs="unbounded"/>
			<xsd:element ref="ustep" maxOccurs="unbounded"/>
			<xsd:element ref="punkt" maxOccurs="unbounded"/>
			<xsd:element ref="litera" maxOccurs="unbounded"/>
			<xsd:element ref="tiret" maxOccurs="unbounded"/>
			<xsd:element ref="akapit" maxOccurs="unbounded"/>
			<xsd:element ref="ustep-przypisu" maxOccurs="unbounded"/>
			<xsd:element ref="punkt-przypisu" maxOccurs="unbounded"/>
			<xsd:element ref="litera-przypisu" maxOccurs="unbounded"/>
			<xsd:element ref="tiret-przypisu" maxOccurs="unbounded"/>
			<xsd:element ref="tabela" maxOccurs="unbounded"/>
			<xsd:element ref="wiersz" maxOccurs="unbounded"/>
			<xsd:element ref="zalacznik" maxOccurs="unbounded"/>
		</xsd:choice>
	</xsd:complexType>
	<xsd:group name="elem-nowelizacyjne">
		<xsd:annotation>
			<xsd:documentation>Elementy nowelizacyjne.</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element ref="dodaj"/>
			<xsd:element ref="uchyl"/>
			<xsd:element ref="zastap"/>
		</xsd:choice>
	</xsd:group>
	<xsd:group name="elem-komentarza">
		<xsd:annotation>
			<xsd:documentation>Elementy komentarza.</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element ref="uwaga" minOccurs="0"/>
			<xsd:element ref="wyspa" minOccurs="0"/>
		</xsd:choice>
	</xsd:group>
	<xsd:group name="elem-formatowania">
		<xsd:annotation>
			<xsd:documentation>Elementy formatowania.</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element ref="wytluszczenie"/>
			<xsd:element ref="kursywa"/>
			<xsd:element ref="podkreslenie"/>
		</xsd:choice>
	</xsd:group>
	<xsd:group name="elem-grupujace">
		<xsd:annotation>
			<xsd:documentation>Elementy grupujace.</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element ref="fragment"/>
			<xsd:element ref="blok"/>
		</xsd:choice>
	</xsd:group>
	<xsd:group name="elem-tekstowe">
		<xsd:annotation>
			<xsd:documentation>Elementy dozwolone w "zwyklym" tekscie (formatowanie i indeksy).</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:group ref="elem-formatowania"/>
			<xsd:element ref="indeks-dolny" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="indeks-gorny" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="elem-grupowe">
		<xsd:annotation>
			<xsd:documentation>Rozszerzenie zwyklego tekstu o elementy grupujace.</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:group ref="elem-tekstowe"/>
			<xsd:group ref="elem-grupujace"/>
		</xsd:choice>
	</xsd:group>
	<xsd:group name="elem-wewnetrzne">
		<xsd:annotation>
			<xsd:documentation>Rozszerzenie elementow grupowych o reprezentacje powiazan.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:group ref="elem-grupowe" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="przypis" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="definicja" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="link" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="przywolanie" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="rozsz-elem-wewnetrzne">
		<xsd:annotation>
			<xsd:documentation>Rozszerzenie elementow wewnetrznych o znaczniki nowelizacyjne, cytowanie i tabele.</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:group ref="elem-wewnetrzne"/>
			<xsd:group ref="elem-nowelizacyjne"/>
			<xsd:element ref="cytuj">
				<xsd:annotation>
					<xsd:documentation>Element umozliwiajacy cytowanie bezpo�rednie przepisow.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!--
				<xsd:element ref="cytowanie">
					<xsd:annotation>
						<xsd:documentation>Element umozliwiajacy cytowanie referencyjne przepisow.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				-->
			<xsd:element ref="cytowanie"/>
			<xsd:element ref="tabela"/>
		</xsd:choice>
	</xsd:group>
	<!--  M e t r y c z k a  -->
	<xsd:element name="metryka">
		<xsd:annotation>
			<xsd:documentation>Metryczka aktu, uzywana jako naglowek badz informacja w przywolaniu.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="organ-wydajacy"/>
				<xsd:element ref="tytul" minOccurs="0"/>
				<xsd:element ref="adres-publikacji" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="status-aktu" use="required">
				<xsd:annotation>
					<xsd:documentation>Informacja o stanie prac nad aktem prawnym.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="projekt"/>
						<xsd:enumeration value="uchwalony"/>
						<xsd:enumeration value="przyjety"/>
						<xsd:enumeration value="ogloszony"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="nazwa" use="optional">
				<xsd:annotation>
					<xsd:documentation>Nazwa aktu ("ustawa", "konstytucja", "regulamin" itp.)</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="numer">
				<xsd:annotation>
					<xsd:documentation>Numer aktu prawnego.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="organ-wydajacy" use="optional">
				<xsd:annotation>
					<xsd:documentation>Element reprezentujacy organ wydajacy akt (w dopelniaczu) - uzupelnienie nazwy.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="data" type="data" use="optional">
				<xsd:annotation>
					<xsd:documentation>Data uchwalenia/wydania aktu.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="opis-typu">
				<xsd:annotation>
					<xsd:documentation>Opis aktu prawnego (rozszerzenie numeru aktu).</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="kadencja" type="zakres_lat">
				<xsd:annotation>
					<xsd:documentation>Okreslenie kadencji organu wydajacego.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="organ-wydajacy">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy organ wydajacy akt (w dopelniaczu) - uzupelnienie nazwy.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:sequence>
				<xsd:element ref="przypis" minOccurs="0"/>
			</xsd:sequence>
<!--			<xsd:attribute name="data" type="data" use="required">-->
<!--				<xsd:annotation>-->
<!--					<xsd:documentation>Data uchwalenia/wydania aktu.</xsd:documentation>-->
<!--				</xsd:annotation>-->
<!--			</xsd:attribute>-->
		</xsd:complexType>
	</xsd:element>
	<!--  E l e m e n t y   s t r u k t u r y  -->
	<xsd:element name="tytul">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy tytul aktu.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0">
				<xsd:element ref="przypis"/>
				<xsd:element ref="fragment"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="srodtytul">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy srodtytul.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:sequence>
				<xsd:element ref="przypis"/>
				<xsd:element ref="fragment"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="element_redakcyjny">
		<xsd:annotation>
			<xsd:documentation>Element redakcyjny posiadajacy pelna funkcjonalnosc.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:sequence>
				<xsd:group ref="rozsz-elem-wewnetrzne"/>
				<xsd:element ref="punkt" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="litera" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="tiret" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="akapit" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:attributeGroup ref="atr-wspolne"/>
			<!--			<xsd:choice minOccurs="0" maxOccurs="unbounded">-->
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="preambula">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy preambule.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:sequence>
				<xsd:group ref="rozsz-elem-wewnetrzne"/>
				<!-- dodane na wniosek TK -->
				<xsd:element ref="akapit" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="punkt" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="litera" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="tiret" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:attributeGroup ref="atr-wspolne"/>
			<!--			<xsd:choice minOccurs="0" maxOccurs="unbounded">-->
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="podstawa-prawna">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy podstawe prawna.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:sequence>
				<xsd:group ref="rozsz-elem-wewnetrzne"/>
				<!-- dodane na wniosek TK -->
				<xsd:element ref="akapit" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="punkt" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="litera" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="tiret" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:attributeGroup ref="atr-wspolne"/>
			<!--			<xsd:choice minOccurs="0" maxOccurs="unbounded">-->
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="czesc">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy czesc.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="tytul" minOccurs="0"/>
				<!--				<xsd:choice maxOccurs="unbounded">-->
				<!--					<xsd:sequence>-->
				<xsd:element ref="ksiega" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="tyt" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="rozdzial" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="artykul" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:group ref="elem-komentarza"/>
				<!--					</xsd:sequence>-->
				<!--				</xsd:choice>-->
			</xsd:sequence>
			<xsd:attributeGroup ref="atr-wspolne-nr"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ksiega">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy ksiege.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="tytul" minOccurs="0"/>
				<!--				<xsd:choice maxOccurs="unbounded">-->
				<!--					<xsd:sequence>-->
				<xsd:element ref="tyt" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="artykul" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:group ref="elem-komentarza"/>
				<!--					</xsd:sequence>-->
				<!--				</xsd:choice>-->
			</xsd:sequence>
			<xsd:attributeGroup ref="atr-wspolne-nr"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="tyt">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy tytul (jako czesc struktury).</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="tytul" minOccurs="0"/>
				<!--				<xsd:choice maxOccurs="unbounded">-->
				<!--					<xsd:sequence>-->
				<xsd:element ref="dzial" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="artykul" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="paragraf" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:group ref="elem-komentarza"/>
				<!--					</xsd:sequence>-->
				<!--				</xsd:choice>-->
			</xsd:sequence>
			<xsd:attributeGroup ref="atr-wspolne-nr"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="dzial">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy dzial.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="tytul" minOccurs="0"/>
				<!--				<xsd:choice maxOccurs="unbounded">-->
				<!--					<xsd:sequence>-->
				<xsd:element ref="rozdzial" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="artykul" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="paragraf" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:group ref="elem-komentarza"/>
				<!--					</xsd:sequence>-->
				<!--				</xsd:choice>-->
			</xsd:sequence>
			<xsd:attributeGroup ref="atr-wspolne-nr"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="rozdzial">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy rozdzial.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="tytul" minOccurs="0"/>
				<!--				<xsd:choice maxOccurs="unbounded">-->
				<!--					<xsd:sequence>-->
				<xsd:element ref="oddzial" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="srodtytul" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="artykul" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="paragraf" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:group ref="elem-komentarza"/>
				<!--					</xsd:sequence>-->
				<!--				</xsd:choice>-->
			</xsd:sequence>
			<xsd:attributeGroup ref="atr-wspolne-nr"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="oddzial">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy oddzial.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="tytul" minOccurs="0"/>
				<!--				<xsd:sequence>-->
				<xsd:element ref="artykul" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="paragraf" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:group ref="elem-komentarza"/>
				<!--				</xsd:sequence>-->
			</xsd:sequence>
			<xsd:attributeGroup ref="atr-wspolne-nr"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="artykul">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy artykul.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:sequence>
				<xsd:group ref="rozsz-elem-wewnetrzne"/>
				<xsd:element ref="akapit" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="paragraf" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="ustep" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="punkt" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="podwyliczenie" minOccurs="0"/>
				<xsd:element ref="litera" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="tiret" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="trybunal" minOccurs="0"/>
				<xsd:element ref="przepisy-wykonawcze" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="dyrektywa" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="wejscie-w-zycie" minOccurs="0"/>
				<xsd:element ref="stosowanie" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:group ref="elem-komentarza"/>
			</xsd:sequence>
			<xsd:attributeGroup ref="atr-wspolne-nr"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="paragraf">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy paragraf.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:sequence>
				<xsd:group ref="rozsz-elem-wewnetrzne"/>
				<xsd:element ref="akapit" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="ustep" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="punkt" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="podwyliczenie" minOccurs="0"/>
				<xsd:element ref="litera" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="tiret" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="trybunal" minOccurs="0"/>
				<xsd:element ref="przepisy-wykonawcze" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="dyrektywa" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:group ref="elem-komentarza"/>
			</xsd:sequence>
			<xsd:attributeGroup ref="atr-wspolne-nr"/>
			<xsd:attribute name="symbol-wysrodkowany" default="nie">
				<xsd:annotation>
					<xsd:documentation>Wyb�r po�o�enia symbolu paragrafu w wydruku.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="tak"/>
						<xsd:enumeration value="nie"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="sentencja">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy sentencji.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:sequence>
				<xsd:element ref="tytul" minOccurs="0"/>
				<xsd:group ref="rozsz-elem-wewnetrzne"/>
				<xsd:element ref="akapit" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="ustep" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="paragraf" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="punkt" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="litera" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="tiret" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="trybunal" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="przepisy-wykonawcze" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="dyrektywa" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="wejscie-w-zycie" minOccurs="0"/>
				<xsd:element ref="stosowanie" minOccurs="0"/>
				<xsd:group ref="elem-komentarza"/>
				<xsd:element ref="element_redakcyjny" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attributeGroup ref="atr-wspolne-nr"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ustep">
		<xsd:annotation>
			<xsd:documentation>Element reprezentujacy ustep.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:sequence>
				<xsd:group ref="rozsz-elem-wewnetrzne"/>
				<xsd:element ref="akapit" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="punkt" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="podwyliczenie" minOccurs="0"/>
				<xsd:element ref="litera" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="tiret" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="trybunal" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="przepisy-wykonawcze" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="dyrektywa" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:group ref="elem-komentarza"/>
			</xsd:sequence>
